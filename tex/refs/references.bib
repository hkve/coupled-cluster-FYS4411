  keywords = {Nanoparticles, Quantum dots, Self-assembly},
  pages = {535--542},
  file = {Full Text PDF:/home/hakon/Zotero/storage/VLESC3G5/Cherniukh et al. - 2021 - Perovskite-type superlattices from lead halide per.pdf:application/pdf},
