  keywords = {High Energy Physics - Theory, Mathematical Physics, Quantum Physics},
  pages = {052209},
  file = {arXiv Fulltext PDF:/home/hakon/Zotero/storage/WQMRUA5M/Ferialdi and DiÃ³si - 2021 - General Wick's Theorem for bosonic and fermionic o.pdf:application/pdf},
}

